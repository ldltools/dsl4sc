// accumulator

event dayclose,knockout,strikeout,dayopen
protocol ((dayopen + expire); dayclose;(knockout + (!knockout; (strikeout + !strikeout) )))*;;

proposition knockouted,expired,struckout
property 
[{true}*][{knockouted}]last;
[{true}*][{expired}]last;
[{true}*][{knockouted}][{true}*]knockouted;
[{true}*][{expired}][{true}*]expired;

rule
on knockout when { _event.data.price &gt; _data.amount[0]} do knockouted;
// on stikeout when { _event.data.price < _data.amount[1]} do struckout;
// on expire when true do expired;
